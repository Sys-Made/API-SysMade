<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.api_sysmade">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the "MyLocation" functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityTelaSocioMenu"
            android:label="                               Sócios"></activity>
        <activity
            android:name=".ActivityConsultaSocio"
            android:label="                     Consultar sócios" />
        <activity
            android:name=".ActivityConsultaCliente"
            android:label="                     Consultar clientes" />
        <activity
            android:name=".ActivityCadastraCliente"
            android:label="                 Cadastrar novo cliente?" />
        <activity
            android:name=".ActivityTelaSobre"
            android:label="                                 Sobre" />
        <activity
            android:name=".ActivityCadastraSocio"
            android:label="                   Cadastrar novo sócio?" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".ActivityMapa"
            android:label="                       Onde estamos!" />
        <activity
            android:name=".ActivityTelaClienteMenu"
            android:label="                       Menu de Clientes" />
        <activity
            android:name=".ActivityTelaMenu"
            android:label="                                 Menu" />
        <activity
            android:name=".ActivityTelaLogin"
            android:label="                                Login" />
        <activity
            android:name=".ActivityTelaServicos"
            android:label="                              Serviços" />
        <activity
            android:name=".ActivityTelaSplash"
            android:label="                   SysMade versão 1.0">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>