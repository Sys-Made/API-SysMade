<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ActivityTelaServicos">

    <Button
        android:id="@+id/buttonVoltarServicos"
        android:layout_width="56dp"
        android:layout_height="51dp"
        android:layout_marginStart="170dp"
        android:layout_marginEnd="181dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/icone_voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="958dp"
        android:layout_height="543dp"
        android:layout_marginBottom="300dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonVoltarServicos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_servicos1" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="958dp"
        android:layout_height="543dp"
        android:layout_marginBottom="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@mipmap/ic_servicos2" />

</androidx.constraintlayout.widget.ConstraintLayout>