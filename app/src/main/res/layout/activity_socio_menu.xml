<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ActivityTelaSocioMenu"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/buttonChamaConsulSocio"
        android:layout_width="260dp"
        android:layout_height="81dp"
        android:layout_marginStart="73dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="59dp"
        android:layout_weight="1"
        android:background="@drawable/button_beauty_full"
        android:text="Consultar"
        app:layout_constraintBottom_toTopOf="@+id/buttonVoltarSocioMenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonChamaCadSocio" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="207dp"
        android:layout_height="171dp"
        android:layout_marginStart="102dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="51dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonChamaCadSocio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher" />

    <Button
        android:id="@+id/buttonChamaCadSocio"
        android:layout_width="256dp"
        android:layout_height="80dp"
        android:layout_marginStart="77dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="24dp"
        android:layout_weight="1"
        android:background="@drawable/button_beauty_full"
        android:text="Cadastrar"
        app:layout_constraintBottom_toTopOf="@+id/buttonChamaConsulSocio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <Button
        android:id="@+id/buttonVoltarSocioMenu"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="175dp"
        android:layout_marginEnd="171dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/icone_voltar"
        android:padding="5dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonChamaConsulSocio" />
</androidx.constraintlayout.widget.ConstraintLayout>