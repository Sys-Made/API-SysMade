<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ActivityTelaMenu">

    <Button
        android:id="@+id/buttonServicos"
        android:layout_width="73dp"
        android:layout_height="67dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="98dp"
        android:background="@drawable/icone_servicos"
        app:layout_constraintBottom_toTopOf="@+id/buttonSobre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonSocio"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtMenu"
        android:layout_width="149dp"
        android:layout_height="57dp"
        android:layout_marginStart="175dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="118dp"
        android:background="@drawable/icone_menu"
        android:fontFamily="sans-serif-black"
        android:includeFontPadding="false"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="40sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonCliente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonSair"
        android:layout_width="67dp"
        android:layout_height="62dp"
        android:layout_marginStart="172dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/icone_sair"
        android:padding="5dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCliente" />

    <Button
        android:id="@+id/buttonSocio"
        android:layout_width="70dp"
        android:layout_height="63dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="193dp"
        android:layout_marginBottom="102dp"
        android:background="@drawable/icone_socios"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonMapa"
        app:layout_constraintEnd_toStartOf="@+id/buttonServicos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonCliente"
        android:layout_width="79dp"
        android:layout_height="76dp"
        android:layout_marginStart="171dp"
        android:layout_marginEnd="171dp"
        android:layout_marginBottom="273dp"
        android:background="@drawable/icone_clientes"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonSair"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtMenu" />

    <Button
        android:id="@+id/buttonMapa"
        android:layout_width="69dp"
        android:layout_height="66dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="194dp"
        android:layout_marginBottom="258dp"
        android:background="@drawable/icone_maps"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonSobre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonSocio" />

    <Button
        android:id="@+id/buttonSobre"
        android:layout_width="73dp"
        android:layout_height="67dp"
        android:layout_marginTop="278dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="257dp"
        android:background="@drawable/icone_sobre"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonMapa"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>