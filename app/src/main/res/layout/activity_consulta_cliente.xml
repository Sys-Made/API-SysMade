<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintConsulCli"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ActivityConsultaCliente">

    <ListView
        android:id="@+id/listViewCliente"
        android:layout_width="409dp"
        android:layout_height="510dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="9dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/buttonNovoCli"
        android:layout_width="138dp"
        android:layout_height="53dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/button_beauty_full"
        android:text="Novo Cliente ++"
        app:layout_constraintBottom_toTopOf="@+id/listViewCliente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonVoltarConsulCli"
        android:layout_width="76dp"
        android:layout_height="55dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/icone_voltar"
        app:layout_constraintBottom_toTopOf="@+id/listViewCliente"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="94dp"
        android:layout_height="66dp"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toTopOf="@+id/listViewCliente"
        app:layout_constraintEnd_toStartOf="@+id/buttonNovoCli"
        app:layout_constraintStart_toEndOf="@+id/buttonVoltarConsulCli"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher" />

</androidx.constraintlayout.widget.ConstraintLayout>