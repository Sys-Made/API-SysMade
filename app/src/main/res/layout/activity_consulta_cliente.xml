<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ActivityConsultaCliente">

    <Button
        android:id="@+id/buttonVoltarConsulCli"
        android:layout_width="59dp"
        android:layout_height="57dp"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="138dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/icone_voltar"
        android:padding="5dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonNovoCli"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/_dynamic" />

    <Button
        android:id="@+id/buttonNovoCli"
        android:layout_width="178dp"
        android:layout_height="53dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="17dp"
        android:layout_weight="1"
        android:background="@drawable/button_beauty_full"
        android:text="Novo Cliente ++"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/buttonVoltarConsulCli"
        app:layout_constraintTop_toBottomOf="@+id/_dynamic" />

    <ImageView
        android:id="@+id/imageViewLogoConsulCli"
        android:layout_width="127dp"
        android:layout_height="80dp"
        android:layout_marginStart="142dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="142dp"
        app:layout_constraintBottom_toTopOf="@+id/_dynamic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher"
        android:contentDescription="@string/todo" />

    <ListView
        android:id="@+id/listViewCliente"
        android:layout_width="403dp"
        android:layout_height="430dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonVoltarConsulCli"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewLogoConsulCli" />

</androidx.constraintlayout.widget.ConstraintLayout>